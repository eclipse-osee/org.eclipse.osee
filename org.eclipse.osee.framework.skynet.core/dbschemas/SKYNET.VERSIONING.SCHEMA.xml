	<Table name="OSEE_DEFINE_ARTIFACT" schema="OSEE">
		<Column id="ART_ID" defaultValue="not null" type="INTEGER" />
		<Column id="ART_TYPE_ID" defaultValue="not null" type="INTEGER" />
		<Column id="GUID" defaultValue="not null" limits="28" type="VARCHAR" />
		<Column id="HUMAN_READABLE_ID" defaultValue="not null" limits="10" type="VARCHAR" />
		<Constraint schema="OSEE" id="OSEE_ARTIFACT_A_PK" type="PRIMARY KEY" appliesTo="ART_ID" />	
		<Constraint schema="OSEE" id="ART_TYPE_ID1_FK" type="FOREIGN KEY" appliesTo="ART_TYPE_ID" deferrable="true">
			<References schema="OSEE" table="OSEE_DEFINE_ARTIFACT_TYPE" column="ART_TYPE_ID" onDelete="CASCADE" />
		</Constraint>
		<Index id="OSEE_ARTIFACT_H_A_IDX" type="UNIQUE">
			<AppliesTo id="HUMAN_READABLE_ID"/>
			<AppliesTo id="ART_ID"/>
		</Index>
		<Index id="OSEE_ARTIFACT_AT_IDX">
			<AppliesTo id="ART_TYPE_ID"/>
		</Index>
		<Index id="OSEE_ARTIFACT_G_IDX" type="UNIQUE">
			<AppliesTo id="GUID"/>
		</Index>
	</Table>
	
	<Table name="OSEE_DEFINE_ARTIFACT_VERSION" schema="OSEE">
		<Column id="ART_ID" defaultValue="not null" type="INTEGER" />
		<Column id="GAMMA_ID" defaultValue="not null" type="BIGINT" />
		<Column id="MODIFICATION_ID" defaultValue="not null" type="INTEGER" />
		<Constraint schema="OSEE" id="OSEE_ARTIFACT_VER_G_PK" type="PRIMARY KEY" appliesTo="GAMMA_ID" />
		<Constraint schema="OSEE" id="ART_ID2_FK" type="FOREIGN KEY" appliesTo="ART_ID" deferrable="true">
			<References schema="OSEE" table="OSEE_DEFINE_ARTIFACT" column="ART_ID" onDelete="CASCADE" />
		</Constraint>
		<Index id="OSEE_ARTIFACT_VER_AR_G_IDX" type="UNIQUE">
			<AppliesTo id="ART_ID"/>
			<AppliesTo id="GAMMA_ID"/>
		</Index>
		<Index id="OSEE_ARTIFACT_VER_A_IDX">
			<AppliesTo id="ART_ID"/>
		</Index>
	</Table>

	<Table name="OSEE_DEFINE_ARTIFACT_TYPE" schema="OSEE">
		<Column id="ART_TYPE_ID" defaultValue="not null" type="INTEGER" />
		<Column id="NAMESPACE" limits="120" type="VARCHAR" />
		<Column id="NAME" defaultValue="not null" limits="75" type="VARCHAR" />
		<Column id="FACTORY_ID" defaultValue="not null" type="INTEGER" />
		<Column id="FACTORY_KEY" defaultValue="not null" limits="100" type="VARCHAR" />
	   <Column id="IMAGE" type="BLOB" />
		<Constraint schema="OSEE" id="OSEE_ARTIFACT_TYPE_AR_PK" type="PRIMARY KEY" appliesTo="ART_TYPE_ID" />
		<Constraint schema="OSEE" id="FACTORY_ID_FK" type="FOREIGN KEY" appliesTo="FACTORY_ID" deferrable="true">
			<References schema="OSEE" table="OSEE_DEFINE_FACTORY" column="FACTORY_ID" onDelete="CASCADE" />
		</Constraint>
		<Index id="OSEE_ARTIFACT_TYPE_N_IDX" type="UNIQUE">
			<AppliesTo id="NAME"/>
		</Index>
	</Table>

	<Table name="OSEE_ATTRIBUTE" schema="OSEE">
		<Column id="GUID" defaultValue="not null" limits="28" type="VARCHAR" />
		<Column id="ATTR_ID" defaultValue="not null" type="INTEGER" />
	</Table>

	<Table name="OSEE_DEFINE_ATTRIBUTE" schema="OSEE">
		<Column id="GAMMA_ID" defaultValue="not null" type="BIGINT" />
		<Column id="ART_ID" defaultValue="not null" type="INTEGER" />
		<Column id="ATTR_ID" defaultValue="not null" type="INTEGER" />
		<Column id="ATTR_TYPE_ID" defaultValue="not null" type="INTEGER" />
		<Column id="VALUE" limits="4000" type="VARCHAR" />
		<Column id="CONTENT" type="BLOB" />
		<Column id="MODIFICATION_ID" defaultValue="not null" type="INTEGER" />
		<Constraint schema="OSEE" id="OSEE_ATTRIBUTE_AT_G_PK" type="PRIMARY KEY" appliesTo="ATTR_ID, GAMMA_ID" />
		<Constraint schema="OSEE" id="ART_ID1_FK" type="FOREIGN KEY" appliesTo="ART_ID" deferrable="true">
			<References schema="OSEE" table="OSEE_DEFINE_ARTIFACT" column="ART_ID" onDelete="CASCADE" />
		</Constraint>
		<Index id="OSEE_ATTRIBUTE_ATT_IDX">
			<AppliesTo id="ATTR_TYPE_ID"/>
		</Index>
 	  	<Index id="OSEE_ATTRIBUTE_V_IDX" mySqlIgnore="true">
			<AppliesTo id="VALUE"/>
		</Index>
		<Index id="OSEE_ATTRIBUTE_V_AR_IDX" mySqlIgnore="true">
			<AppliesTo id="ART_ID"/>
			<AppliesTo id="VALUE"/>
		</Index>
		<Index id="OSEE_ATTRIBUTE_V_AR_IDX" mySqlIgnore="true">
			<AppliesTo id="VALUE"/>
			<AppliesTo id="ART_ID"/>
		</Index>
		<Index id="OSEE_ATTRIBUTE_AR_G_IDX" type="UNIQUE">
			<AppliesTo id="ART_ID"/>
			<AppliesTo id="GAMMA_ID"/>
		</Index>
		<Index id="OSEE_ATTRIBUTE_G_AT_IDX" type="UNIQUE">
			<AppliesTo id="GAMMA_ID"/>
			<AppliesTo id="ATTR_ID"/>
		</Index>
		<Index id="OSEE_ATTRIBUTE_G_IDX" type="UNIQUE">
			<AppliesTo id="GAMMA_ID"/>
		</Index>
		<Index id="OSEE_ATTRIBUTE_AT_IDX">
			<AppliesTo id="ATTR_ID"/>
		</Index>
		<Index id="OSEE_ATTRIBUTE_AR_IDX" mySqlIgnore="true">
			<AppliesTo id="ART_ID"/>
		</Index>
	</Table>
	
	<Table name="OSEE_DEFINE_ATTRIBUTE_TYPE" schema="OSEE">
		<Column id="ATTR_TYPE_ID" defaultValue="not null" type="INTEGER" />
		<Column id="ATTR_BASE_TYPE_ID" defaultValue="not null" type="INTEGER" />
		<Column id="NAMESPACE" limits="120" type="VARCHAR" />
		<Column id="NAME" defaultValue="not null" limits="500" type="VARCHAR" />
		<Column id="DEFAULT_VALUE" limits="500" type="VARCHAR" />
		<Column id="VALIDITY_XML" limits="4000" type="VARCHAR" />
		<Column id="MIN_OCCURENCE" defaultValue="not null" type="INTEGER" />
		<Column id="MAX_OCCURENCE" defaultValue="not null" type="INTEGER" />
		<Column id="TIP_TEXT" limits="4000" type="VARCHAR" />
		<Constraint schema="OSEE" id="OSEE_ATTRIBUTE_TYPE_AT_PK" type="PRIMARY KEY" appliesTo="ATTR_TYPE_ID" />
		<Constraint schema="OSEE" id="ATTR_BASE_TYPE_ID_FK" type="FOREIGN KEY" appliesTo="ATTR_BASE_TYPE_ID" deferrable="true">
			<References schema="OSEE" table="OSEE_DEFINE_ATTR_BASE_TYPE" column="ATTR_BASE_TYPE_ID" onDelete="CASCADE" />
		</Constraint>
		<Index id="OSEE_ATTRIBUTE_TYPE_N_IDX">
			<AppliesTo id="NAME"/>
		</Index>
	</Table>

	<Table name="OSEE_DEFINE_ATTR_BASE_TYPE" schema="OSEE">
		<Column id="ATTR_BASE_TYPE_ID" defaultValue="not null" type="INTEGER" />
		<Column id="ATTRIBUTE_CLASS" defaultValue="not null" limits="500" type="VARCHAR" />
		<Constraint schema="OSEE" id="OSEE_ATTR_BASE_TYPE_AT_PK" type="PRIMARY KEY" appliesTo="ATTR_BASE_TYPE_ID" />
		<Index id="OSEE_ATTR_BASE_TYPE_AC_IDX" type="UNIQUE">
			<AppliesTo id="ATTRIBUTE_CLASS"/>
		</Index>
	</Table>
	
	<Table name="OSEE_DEFINE_FACTORY" schema="OSEE">
		<Column id="FACTORY_ID" defaultValue="not null" type="INTEGER" />
		<Column id="FACTORY_CLASS" defaultValue="not null" limits="500" type="VARCHAR" />
		<Constraint schema="OSEE" id="OSEE_FACTORY_FI_PK" type="PRIMARY KEY" appliesTo="FACTORY_ID" />
		<Index id="OSEE_FACTORY_FC_IDX" type="UNIQUE">
			<AppliesTo id="FACTORY_CLASS"/>
		</Index>
	</Table>
	
	<Table name="OSEE_REL_LINK" schema="OSEE">
		<Column id="GUID" defaultValue="not null" limits="28" type="VARCHAR" />
		<Column id="REL_LINK_ID" defaultValue="not null" type="INTEGER" />
	</Table>

	<Table name="OSEE_DEFINE_REL_LINK" schema="OSEE">
		<Column id="GAMMA_ID" defaultValue="not null" type="BIGINT" />
		<Column id="REL_LINK_ID" defaultValue="not null" type="INTEGER" />
		<Column id="REL_LINK_TYPE_ID" defaultValue="not null" type="INTEGER" />
		<Column id="A_ART_ID" defaultValue="not null" type="INTEGER" />
		<Column id="B_ART_ID" defaultValue="not null" type="INTEGER" />
		<Column id="A_ORDER_VALUE" type="INTEGER" />
		<Column id="B_ORDER_VALUE" type="INTEGER" />
		<Column id="RATIONALE" limits="4000" type="VARCHAR" />
		<Column id="MODIFICATION_ID" defaultValue="not null" type="INTEGER" />
		<Constraint schema="OSEE" id="OSEE_REL_LINK_G_PK" type="PRIMARY KEY" appliesTo="GAMMA_ID" />
		<Constraint schema="OSEE" id="OSEE_FOREIGN_B_ART" type="FOREIGN KEY" appliesTo="B_ART_ID" deferrable="true">
			<References schema="OSEE" table="OSEE_DEFINE_ARTIFACT" column="ART_ID" onDelete="CASCADE" />
		</Constraint>
		<Constraint schema="OSEE" id="OSEE_DEFINE_FOREIGN_A_ART" type="FOREIGN KEY" appliesTo="A_ART_ID" deferrable="true">
			<References schema="OSEE" table="OSEE_DEFINE_ARTIFACT" column="ART_ID" onDelete="CASCADE" />
		</Constraint>
		<Index id="OSEE_REL_LINK_RL_IDX">
			<AppliesTo id="REL_LINK_ID"/>
		</Index>
		<Index id="OSEE_REL_LINK_RL_G_IDX" type="UNIQUE">
			<AppliesTo id="REL_LINK_ID"/>
			<AppliesTo id="GAMMA_ID"/>
		</Index>
		<Index id="OSEE_REL_LINK_AA_IDX">
			<AppliesTo id="A_ART_ID"/>
		</Index>
		<Index id="OSEE_REL_LINK_BA_IDX">
			<AppliesTo id="B_ART_ID"/>
		</Index>
		<Index id="OSEE_REL_LINK_BA_RL_IDX">
			<AppliesTo id="B_ART_ID"/>
			<AppliesTo id="REL_LINK_ID"/>
		</Index>
		<Index id="OSEE_REL_LINK_RL_AA_IDX">
			<AppliesTo id="REL_LINK_ID"/>
			<AppliesTo id="A_ART_ID"/>
		</Index>
		<Index id="OSEE_REL_LINK_AA_BA_IDX">
			<AppliesTo id="A_ART_ID"/>
			<AppliesTo id="B_ART_ID"/>
		</Index>
		<Index id="OSEE_REL_LINK_BA_AA_IDX">
			<AppliesTo id="B_ART_ID"/>
			<AppliesTo id="A_ART_ID"/>
		</Index>
		<Index id="OSEE_REL_LINK_AA_G_IDX" type="UNIQUE">
			<AppliesTo id="A_ART_ID"/>
			<AppliesTo id="GAMMA_ID"/>
		</Index>
		<Index id="OSEE_REL_LINK_BA_G_IDX" type="UNIQUE">
			<AppliesTo id="B_ART_ID"/>
			<AppliesTo id="GAMMA_ID"/>
		</Index>
		<Index id="OSEE_REL_LINK_RT_IDX">
			<AppliesTo id="REL_LINK_TYPE_ID"/>
		</Index>
	</Table>
	
	<Table name="OSEE_DEFINE_REL_LINK_TYPE" schema="OSEE">
		<Column id="GAMMA_ID" defaultValue="not null" type="BIGINT" />
		<Column id="REL_LINK_TYPE_ID" defaultValue="not null" type="INTEGER" />
		<Column id="NAMESPACE" limits="120" type="VARCHAR" />
		<Column id="TYPE_NAME" defaultValue="not null" limits="50" type="VARCHAR" />
		<Column id="A_NAME" defaultValue="not null" limits="50" type="VARCHAR" />
		<Column id="B_NAME" defaultValue="not null" limits="50" type="VARCHAR" />
		<Column id="AB_PHRASING" defaultValue="not null" limits="50" type="VARCHAR" />
		<Column id="BA_PHRASING" defaultValue="not null" limits="50" type="VARCHAR" />
		<Column id="SHORT_NAME" defaultValue="not null" limits="20" type="VARCHAR" />
		<Constraint schema="OSEE" id="OSEE_REL_LINK_TYPE_RLT_PK" type="PRIMARY KEY" appliesTo="REL_LINK_TYPE_ID" />
		<Index id="OSEE_REL_LINK_TYPE_TN_IDX" type="UNIQUE">
			<AppliesTo id="TYPE_NAME"/>
		</Index>
	</Table>
	
	<Table name="OSEE_DEFINE_VALID_ATTRIBUTES" schema="OSEE">
		<Column id="ART_TYPE_ID" defaultValue="not null" type="INTEGER" />
		<Column id="ATTR_TYPE_ID" defaultValue="not null" type="INTEGER" />
		<Constraint schema="OSEE" id="OSEE_VALID_ATTR_AR_AT_PK" type="PRIMARY KEY" appliesTo="ART_TYPE_ID, ATTR_TYPE_ID" />
		<Constraint schema="OSEE" id="ATTR_TYPE_ID_FK" type="FOREIGN KEY" appliesTo="ATTR_TYPE_ID" deferrable="true">
			<References schema="OSEE" table="OSEE_DEFINE_ATTRIBUTE_TYPE" column="ATTR_TYPE_ID" onDelete="CASCADE" />
		</Constraint>
		<Constraint schema="OSEE" id="ART_TYPE_ID2_FK" type="FOREIGN KEY" appliesTo="ART_TYPE_ID" deferrable="true">
			<References schema="OSEE" table="OSEE_DEFINE_ARTIFACT_TYPE" column="ART_TYPE_ID" onDelete="CASCADE" />
		</Constraint>
	</Table>

	<Table name="OSEE_DEFINE_VALID_RELATIONS" schema="OSEE">
		<Column id="GAMMA_ID" defaultValue="not null" type="BIGINT" />
		<Column id="SIDE_A_MAX" defaultValue="not null" type="INTEGER" />
		<Column id="SIDE_B_MAX" defaultValue="not null" type="INTEGER" />
		<Column id="ART_TYPE_ID" defaultValue="not null" type="INTEGER" />
		<Column id="REL_LINK_TYPE_ID" defaultValue="not null" type="INTEGER" />
		<Constraint schema="OSEE" id="OSEE_VALID_REL_AT_RT_G_PK" type="PRIMARY KEY" appliesTo="ART_TYPE_ID, REL_LINK_TYPE_ID, GAMMA_ID" />
		<Constraint schema="OSEE" id="ODVR__GAMMA_ID_UN" type="UNIQUE" appliesTo="GAMMA_ID" />
	</Table>
	
	<Table name="OSEE_SNAPSHOT" schema="OSEE">
		<Column id="NAMESPACE" defaultValue="not null" limits="1000" type="VARCHAR" />
		<Column id="KEY" defaultValue="not null" limits="1000" type="VARCHAR" />
		<Column id="OBJECT" type="BLOB" />
		<Column id="LAST_UPDATED" defaultValue="not null" type="TIMESTAMP" />
		<Column id="LAST_ACCESSED" defaultValue="not null" type="TIMESTAMP" />
		<Constraint schema="OSEE" id="OSEE_SNAPSHOT__K_N_PK" type="PRIMARY KEY" appliesTo="KEY, NAMESPACE" />
	</Table>

